do {
let text = "foo\nbar\n\r\nbaz\n"
let lines = lines(text)
let expected = ["foo", "bar", "", "baz"]

print(lines)
print(expected)

let len = list_len(lines)
assert_eq(len, list_len(expected))

let i = 0
loop {
  if i == len {
    break
  }

  assert_eq(lines[i], expected[i])

  i += 1
}
}

