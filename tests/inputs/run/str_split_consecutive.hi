
// Split with consecutive separators (creates empty segments)
let text = "foo,,bar"
let result = split(text, ",")
let expected = ["foo", "", "bar"]

print(result)
print(expected)

let len = list_len(result)
assert_eq(len, list_len(expected))

let i = 0
loop {
  if i == len {
    break
  }
  assert_eq(result[i], expected[i])
  i += 1
}
